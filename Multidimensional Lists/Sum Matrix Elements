row, col = [int(el) for el in input().split(', ')]
matrix= []
for i in range(row):
    inner_list = [int(el) for el in input().split(',')]
    matrix.append(inner_list)
sum_nums = 0
for row_index in range(row):
    for col_index in range(col):
        sum_nums += matrix[row_index][col_index]

print(sum_nums)
print(matrix)
